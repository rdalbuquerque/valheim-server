{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach to Go Function",
            "type": "go",
            "request": "attach",
            "mode": "remote",
            "remotePath": "${workspaceFolder}",
            "port": 2345,
            "host": "127.0.0.1",
            "apiVersion": 2,
            "trace": "log"
        },
        {
            "name": "Debug Go Tests",
            "type": "go",
            "request": "launch",
            "mode": "test",
            "envFile": "${workspaceFolder}/discordbot/.env"
        }
    ]
}
